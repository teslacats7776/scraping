<div class="scrape-page-bg">

  <div class="container">

    <div class="row">
      <div class="col s12 m12">
        <div class="scrape-page-header black-text center">New York Times Scrapes</div>
        <div class="scrape-page-header2 black-text center">Web Scraping</div>
      </div>
    </div>

    {{#each articles}} {{#unless this.saved}}
    <div class="row">
      <div class="col s12 m12">
        <div class="card">
          <div class="card-panel card-panel-top">
            <span class="white-text">
              <a href={{this.link}} target="_blank">
                <h5>{{this.title}}</h5>
              </a>
            </span>
          </div>
          <div class="card-content white">
            <span class="black-text white">
              <p>{{this.summary}}</p>
              <a data-id="{{this._id}}" class="waves-effect waves-light btn btn-medium right save-article-btn">Save
                Article</a>
            </span>
          </div>
          <div class="card-panel card-panel-bottom"></div>
        </div>
      </div>
    </div>
    {{/unless}} {{/each}}

  </div>

</div>